# API Configuration
API_URL=https://your-worker.pages.dev
API_KEY=your-api-key-here

# API Client Configuration
API_MAX_RETRIES=3
API_RETRY_DELAY=2000
API_USE_SESSION_TOKEN=true

# Schedule Configuration (cron expression)
# Examples:
#   0 * * * *     - Every hour at minute 0
#   */30 * * * *  - Every 30 minutes
#   0 */2 * * *   - Every 2 hours
#   0 0 * * *     - Every day at midnight
CHECK_CRON=0 * * * *

# CFST Configuration
CONCURRENT_TESTS=5

# Sort Mode: speed (download speed, default) or latency
SORT_MODE=speed

# Test Mode: cfip, proxyip, outbound, all
TEST_MODE=cfip

# Enable/Disable Auto Update
ENABLE_AUTO_UPDATE=true

# Telegram Notification (optional)
TG_ENABLED=false
TG_BOT_TOKEN=your-bot-token
TG_CHAT_ID=your-chat-id
TG_PROXY=                     # e.g., http://127.0.0.1:7890 or socks5://127.0.0.1:1080
